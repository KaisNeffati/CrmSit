{% extends "SITPlatformBundle::layout.html.twig" %}

{% block title %}
    Annonces - {{ parent() }}
{% endblock %}

    {% block sitplatform_center %}
        <table cellpadding="0" cellspacing="0" border="0" class="dataTable mdl-data-table mdl-js-data-table mdl-shadow--2dp" id="example">
            <thead>
            <tr>
                <th class="mdl-data-table__cell--non-numeric">Nom</th>
                <th>Prenom</th>
                <th>Email</th>
                <th>cin</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            {% for item in listCommerciaux %}
                <tr>
                    <td>{{ item.nom }}</td>
                    <td>{{ item.prenom }}</td>
                    <td>{{ item.email }}</td>
                    <td>{{ item.cin }}</td>
                    <td><a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" href="{{ path('sit_commercial_rapport',{'idcommercial': item.id }) }}">Rapport</a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
        <script>
            $(document).ready(function() {
                $('#example').dataTable({"sPaginationType": "full_numbers"});
            });
        </script>
    {% endblock %}
