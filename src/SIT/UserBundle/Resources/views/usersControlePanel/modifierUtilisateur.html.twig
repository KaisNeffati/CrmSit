{% extends "SITUserBundle::layoutUsers.html.twig" %}

        {% block title %}
            Annonces - {{ parent() }}
        {% endblock %}
    {% block sitplatform_center %}
        {{ form_start(form) }}
        {{ form_errors(form) }}

        <div class="demo-card-square mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title mdl-card--expand">
                <h2 class="mdl-card__title-text">Modifier Utilisateur : </h2>
            </div>

            {{ include('SITUserBundle:usersControlePanel:formUtilisateur.html.twig') }}

            <div class="mdl-card__actions mdl-card--border">
                <a class="">
                    {{ form_widget(form.enregister, {'attr': {'class': 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect','type':'submit'}}) }}
                </a>
                <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" href="{{ path('sit_utilisateurs') }}">
                    Cancel
                </a>
            </div>
        </div>
        {{ form_end(form) }}
    {% endblock %}

